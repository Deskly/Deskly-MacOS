# platform :ios, '9.0'

target 'Deskly' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Deskly

  target 'DesklyTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'DesklyUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

